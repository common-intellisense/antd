  "props": {
    "code": {
      "default": "false",
      "value": "",
      "type": "boolean",
      "description": "Add code style",
      "description_zh": "添加代码样式"
    },
    ":copyable": {
      "default": "false",
      "value": "",
      "type": "boolean / copyable",
      "description": "Whether to be copyable, customize it via object",
